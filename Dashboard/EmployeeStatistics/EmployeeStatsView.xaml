<UserControl
    x:Class="Dashboard.EmployeeStatistics.EmployeeStatsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Dashboard.EmployeeStatistics"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:inf="clr-namespace:Infrastructure;assembly=Infrastructure"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid Background="#FFEEE94E" >
        <Button Style="{StaticResource ManageButtonStyle}" CommandParameter="Employees" Command="{x:Static inf:GlobalCommands.NavigateToManageEntityViewCompositeCommand}" ></Button>
    </Grid>
</UserControl>
